{
  "version": 1,
  "projects": {
    "core-entities": {
      "projectType": "library",
      "root": "libs/core/entities",
      "sourceRoot": "libs/core/entities/src",
      "prefix": "seek-stream",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/core/entities/tsconfig.lib.json",
            "project": "libs/core/entities/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/entities/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core/entities/tsconfig.lib.json",
              "libs/core/entities/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/core/entities/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/entities/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "server-player": {
      "root": "apps/server/player",
      "sourceRoot": "apps/server/player/src",
      "projectType": "application",
      "prefix": "server-player",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/server/player",
            "main": "apps/server/player/src/main.ts",
            "tsConfig": "apps/server/player/tsconfig.app.json",
            "assets": [
              "apps/server/player/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/player/src/environments/environment.ts",
                  "with": "apps/server/player/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "server-player:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/server/player/tsconfig.app.json",
              "apps/server/player/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/server/player/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/server/player/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "core-domain": {
      "root": "libs/core/domain",
      "sourceRoot": "libs/core/domain/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core/domain/tsconfig.lib.json",
              "libs/core/domain/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/core/domain/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/domain/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/core/domain",
            "tsConfig": "libs/core/domain/tsconfig.lib.json",
            "packageJson": "libs/core/domain/package.json",
            "main": "libs/core/domain/src/index.ts",
            "assets": [
              "libs/core/domain/*.md"
            ]
          }
        }
      }
    },
    "web-audio": {
      "projectType": "library",
      "root": "libs/web/audio",
      "sourceRoot": "libs/web/audio/src",
      "prefix": "seek",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/web/audio/tsconfig.lib.json",
            "project": "libs/web/audio/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/web/audio/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/web/audio/tsconfig.lib.json",
              "libs/web/audio/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/web/audio/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/web/audio/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "demo-audio": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo/audio",
      "sourceRoot": "apps/demo/audio/src",
      "prefix": "demo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo/audio",
            "index": "apps/demo/audio/src/index.html",
            "main": "apps/demo/audio/src/main.ts",
            "polyfills": "apps/demo/audio/src/polyfills.ts",
            "tsConfig": "apps/demo/audio/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/demo/audio/src/favicon.ico",
              "apps/demo/audio/src/assets"
            ],
            "styles": [
              "apps/demo/audio/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/audio/src/environments/environment.ts",
                  "with": "apps/demo/audio/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-audio:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-audio:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-audio:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/audio/tsconfig.app.json",
              "apps/demo/audio/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/demo/audio/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo/audio/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-audio-e2e": {
      "root": "apps/demo/audio-e2e",
      "sourceRoot": "apps/demo/audio-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo/audio-e2e/cypress.json",
            "tsConfig": "apps/demo/audio-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo-audio:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-audio:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/audio-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/demo/audio-e2e/**/*"
            ]
          }
        }
      }
    },
    "demo-multimedia": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo/multimedia",
      "sourceRoot": "apps/demo/multimedia/src",
      "prefix": "seek-stream",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/demo/multimedia",
            "index": "apps/demo/multimedia/src/index.html",
            "main": "apps/demo/multimedia/src/main.ts",
            "polyfills": "apps/demo/multimedia/src/polyfills.ts",
            "tsConfig": "apps/demo/multimedia/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/demo/multimedia/src/favicon.ico",
              "apps/demo/multimedia/src/assets",
              "apps/demo/multimedia/src/manifest.webmanifest"
            ],
            "styles": [
              "apps/demo/multimedia/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "hammerjs",
              "socket.io-client"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/multimedia/src/environments/environment.ts",
                  "with": "apps/demo/multimedia/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/demo/multimedia/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-multimedia:build",
            "proxyConfig": "apps/demo/multimedia/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-multimedia:build:production"
            }
          }
        },
        "with-server": {
          "builder": "@angular-devkit/architect:allOf",
          "options": {
            "targets": [
              {
                "target": "demo-multimedia:serve"
              },
              {
                "target": "server-player:serve"
              }
            ]
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-multimedia:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/multimedia/tsconfig.app.json",
              "apps/demo/multimedia/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/demo/multimedia/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo/multimedia/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "demo-multimedia-e2e": {
      "root": "apps/demo/multimedia-e2e",
      "sourceRoot": "apps/demo/multimedia-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo/multimedia-e2e/cypress.json",
            "tsConfig": "apps/demo/multimedia-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo-multimedia:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-multimedia:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/multimedia-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/demo/multimedia-e2e/**/*"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "demo-multimedia"
}
